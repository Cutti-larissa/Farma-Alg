program FA02E022;
var
    x,y:integer;
begin
    read(x,y);
    if (x>0) and (y>0) then
        write(1);
    if (x<0) and (y>0) then
        write(2);
    if (x<0) and (y<0) then
        write(3);
    if (x>0) and (y<0) then
        write(4);
    if (x=0) and (y<>0) then
        write('Y');
    if (x<>0) and (y=0) then
        write('X');
    if (x=0) and (y=0) then
        write('O');
end.
